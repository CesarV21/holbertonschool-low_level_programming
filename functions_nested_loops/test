#include "main.h"
/**
* print_alphabet - show the alphabe ten times
*
* Return: Always
* @void:print_alphabet_x10
*/

void print_alphabet_x10(void)


{
c c;
{
for (c = 'a'; c <= 'z'; c++)

_putchar(c*10);
}
_putchar('\n');
}
~                                                                                                                    
